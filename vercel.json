{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "fluid": true,
  "functions": {
    "api/index.py": {
      "maxDuration": 300,
      "excludeFiles": "{venv/**,__pycache__/**,*.csv,exa-results.json,debug/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
